extends layout

block content
  h1 Lista de Alunos
  table(border="1")
    tr
      th ID
      th Nome
      each num in [1,2,3,4,5,6,7,8]
        th TPC#{num}
    each aluno in alunos
      tr
        td= aluno._id
        td= aluno.nome
        each num in [1,2,3,4,5,6,7,8]
          td= aluno["tpc" + num] ? "✔️" : "❌"
